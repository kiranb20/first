<%= form_with(model: Userdetail, url: admin_path) do |form| %>
    <div>
      <%= form.label :email %>
      <%= form.text_field :email %>
    </div>
  
    <div>
      <%= form.label :password %>
      <%= form.password_field :password %>
    </div>
  
    <div>
      <%= form.label :role %><br>
      <%= form.radio_button :role, 'student' %>
      <%= form.label :role_student, 'Student' %>
      <%= form.radio_button :role, 'teacher' %>
      <%= form.label :role_teacher, 'Teacher' %>
    </div>
  
    <div>
      <%= form.submit 'Submit' %>
    </div>
  <% end %>
  